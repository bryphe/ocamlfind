{
  "name": "lib-findlib",
  "version": "9.9.9",
  "esy": {
      "build": [
        [
          "./configure",
          "-bindir",
          "#{self.bin}",
          "-sitelib",
          "#{self.lib}",
          "-mandir",
          "#{self.man}",
          "-config",
          "#{self.lib}/findlib.conf",
          "-no-custom",
          "-no-topfind"
        ],
        [
          "make",
          "all"
        ],
        [
          "make",
          "opt"
        ]
      ],
      "install": [
        [
          "make",
          "install"
        ],
        [
          "install",
          "-m",
          "0755",
          "ocaml-stub",
          "#{self.bin}/ocaml"
        ],
        [
          "mkdir",
          "-p",
          "#{self.toplevel}"
        ],
        [
          "install",
          "-m",
          "0644",
          "src/findlib/topfind",
          "#{self.toplevel}/topfind"
        ]
      ],
      "exportedEnv": {
        "OCAML_TOPLEVEL_PATH": {
          "val": "#{self.toplevel}",
          "scope": "global"
        }
      },
      "buildsInSource": true
  },
  "peerDependencies": {
    "ocaml": "~4.6.0"
  },
  "dependencies": {
    "ocaml": "~4.6.0",
    "@opam/conf-m4": "*",
    "@opam/ocamlbuild": "*"
  }
}
